       <!-- Family Member Views -->

        <record id="view_banastech_hms_family_member_tree" model="ir.ui.view">
            <field name="name">banastech.hms.family_member.tree</field>
            <field name="model">banastech.hms.family_member</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <tree string="HMS Family Member">
                    <field name="family_id" />
                    <field name="role" />
                    <field name="member" />
                </tree>
            </field>
        </record>
        <record id="view_banastech_hms_family_member_form" model="ir.ui.view">
            <field name="name">banastech.hms.family_member.form</field>
            <field name="model">banastech.hms.family_member</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <form string="HMS Family Member">
                    <sheet>
                        <group>
                            <group>
                                <field name="family_id" colspan="4" />
                                <field name="member" colspan="4" />
                            </group>
                            <group>
                                <field name="role" colspan="4" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_banastech_hms_family_member_search" model="ir.ui.view">
            <field name="name">banastech.hms.family_member.search</field>
            <field name="model">banastech.hms.family_member</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <search string="HMS Family Member">
                    <field name="role" />
                    <field name="member" />
                    <field name="family_id" />
                    <newline />
                    <group expand="0" string="Group By...">
                        <filter name="member" string="Party" domain="[]" context="{'group_by':'member'}" />
                        <filter name="role" string="Role" domain="[]" context="{'group_by':'role'}" />
                        <filter name="family_id" string="Family" domain="[]" context="{'group_by':'family_id'}" />
                    </group>
                </search>
            </field>
        </record>
        <record model="ir.actions.act_window" id="act_banastech_hms_family_member_view">
            <field name="name">HMS Family Member</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">banastech.hms.family_member</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_banastech_hms_family_member_search" />
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
