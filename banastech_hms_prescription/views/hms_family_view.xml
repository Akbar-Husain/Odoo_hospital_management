<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_banastech_hms_family_tree" model="ir.ui.view">
            <field name="name">banastech.hms.family.tree</field>
            <field name="model">banastech.hms.family</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <tree string="HMS Family">
                    <field name="name" />
                    <field name="operational_sector" />
                </tree>
            </field>
        </record>
        <record id="view_banastech_hms_family_form" model="ir.ui.view">
            <field name="name">banastech.hms.family.form</field>
            <field name="model">banastech.hms.family</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <form string="HMS Family" version="7.0">
                    <sheet>
                        <H1>
                            <label for="name" string="Family: " class="o_form_label"/>
                            <field name="name" />
                        </H1>
                        <notebook>
                            <page string="Main">
                                <group>
                                    <field name="operational_sector">
                                        <tree>
                                            <field name="name" />
                                            <!-- <field name="operational_area_id" /> -->
                                        </tree>
                                        <form>
                                            <field name="name" />
                                            <!-- <field name="operational_area_id" /> -->
                                            <group colspan="4" string="Extra Infomation">
                                                <field name="info"/>
                                            </group>
                                        </form>
                                    </field>
                                </group>
                                <group colspan="4" col="4" string="Members">
                                    <field name="members" colspan="4" nolabel="1">
                                        <tree>
                                            <field name="role" />
                                            <field name="member" />
                                            <field name="family_id" />
                                        </tree>
                                        <form>
                                            <group>
                                                <field name="role" />
                                                <field name="member" />
                                            </group>
                                        </form>
                                    </field>
                                </group>
                            </page>
                            <page string="Info">
                                <group string="Info" colspan="4" col="4">
                                    <field name="info" colspan="4" nolabel="0" />
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="view_banastech_hms_family_search" model="ir.ui.view">
            <field name="name">banastech.hms.family.search</field>
            <field name="model">banastech.hms.family</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <search string="HMS Family">
                    <field name="info"/>
                    <field name="operational_sector"/>
                    <field name="name"/>
                    <newline/>
                    <group expand="0" string="Group By...">
                        <filter name="operational_sector" string="Operational Sector" domain="[]" context="{'group_by':'operational_sector'}" />
                        <filter name="name" string="Family" domain="[]" context="{'group_by':'name'}" />
                    </group>
                </search>
            </field>
        </record>
        <record id="act_open_banastech_hms_family_view" model="ir.actions.act_window">
            <field name="name">HMS Family</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">banastech.hms.family</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_banastech_hms_family_search" />
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
