<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <template id="hms_icu_chart_report">
      <div class="header">
        <div class="row">
          <div class="col-xs-3">
            <img class="img img-responsive" t-if="res_company.logo" t-att-src="'data:image/png;base64,%s' % res_company.logo" style="height: 150px;"/>
          </div>
          <div class="col-xs-8" style="font-size:25px; color:#001c49;">
            ARHAM ICU &amp; TRAUMA CENTER
          </div>
          <div clas="col-xs-1"></div>
        </div>
      </div>

      <t t-foreach="docs" t-as="o">
