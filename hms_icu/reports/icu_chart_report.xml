        <div class="row">
          <div style="font-size:15px; color:#7C7BAD;">
            <p><strong>ICU Patient Details</strong></p>
          </div>
          <table class="table table-condensed table-bordered" style="font-size:12px;">
            <tr colspan="4" style="background-color:#DBE1E5;">
              <td></td>
              <td></td>
              <td></td>
              <td></td>
            </tr>
            <tr>
              <td>Report Printed Date</td>
              <td><span t-esc="context_timestamp(datetime.datetime.now()).strftime('%Y-%m-%d %H:%M')"/></td>
              <td><strong>Patient Name</strong></td>
              <td><span t-field="o.patient_id"/></td>
            </tr>
            <tr>
              <td><strong>Patient ID</strong></td>
              <td><span t-field="o.code"/></td>
              <td><strong>Age</strong></td>
              <td><span t-field="o.age"/></td>
            </tr>
            <tr>
              <td><strong>Sex</strong></td>
              <td><span t-field="o.gender"/></td>
              <td><strong>Blood Group</strong></td>
              <td><span t-field="o.blood_group"/></td>
            </tr>
            <tr>
              <td><strong>Height</strong></td>
              <td><span t-field="o.cm"/></td>
              <td><strong>Weight</strong></td>
              <td><span t-field="o.weight"/></td>
            </tr>
          </table>
        </div>
