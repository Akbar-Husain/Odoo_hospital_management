<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
         <!-- Sequences for Bone Bank -->
        <record id="seq_bone_bank_user" model="ir.sequence">
            <field name="name">SHBB</field>
            <field name="code">bone.bank</field>
            <field name="prefix">BBR</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_bone_box" model="ir.sequence">
            <field name="name">Box</field>
            <field name="code">box.management</field>
            <field name="prefix">BOX</field>
            <field name="padding">3</field>
        </record>

       <record id="seq_bone_donor" model="ir.sequence">
            <field name="name">Box Donor</field>
            <field name="code">bone.donor</field>
            <field name="prefix">KZBD</field>
            <field name="padding">5</field>
        </record>

        <!-- Sequences for Receiver -->
        <record id="seq_bone_receiver" model="ir.sequence">
            <field name="name">Box Receiver</field>
            <field name="code">bone.receiver</field>
            <field name="prefix">KZBR</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
