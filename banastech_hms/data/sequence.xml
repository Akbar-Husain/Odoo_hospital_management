<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="seq_banastech_hms_patient" model="ir.sequence">
            <field name="name">Patient</field>
            <field name="code">banastech.hms.patient</field>
            <field name="prefix">Pt./%(month)s/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_banas_hms_doctor" model="ir.sequence">
            <field name="name">Doctor</field>
            <field name="code">banas.hms.doctor</field>
            <field name="prefix">Dr.</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_banastech_hms_appointment" model="ir.sequence">
            <field name="name">Appointment</field>
            <field name="code">banastech.hms.appointment</field>
            <field name="prefix">Apt./%(month)s/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>