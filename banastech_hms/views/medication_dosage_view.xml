<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_banas_hms_medication_dosage_tree" model="ir.ui.view">
            <field name="name">banas.hms.medication.dosage.tree</field>
            <field name="model">banas.hms.medication.dosage</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <tree string="HMS Medication Dosage">
                    <field name="abbreviation" />
                    <field name="code" />
                    <field name="name" />
                </tree>
            </field>
        </record>
        <record id="view_banas_hms_medication_dosage_form" model="ir.ui.view">
            <field name="name">banas.hms.medication.dosage.form</field>
            <field name="model">banas.hms.medication.dosage</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <form string="HMS Medication Dosage">
                    <sheet>
                        <group>
                            <group>
                                <field name="abbreviation"/>
                                <field name="name"/>
                                <field name="lng_guj"/>
                            </group>
                            <group>
                                <field name="code" />
                                <field name="lng_hindi"/>
                            </group>
                        </group>
                        <group string="Timings" col="4">
                            <field name="t1"></field>
                            <field name="t2"></field>
                            <field name="t3"></field>
                            <field name="t4"></field>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_banas_hms_medicament_context_tree" model="ir.ui.view">
            <field name="name">banas.hms.medicament.content.tree</field>
            <field name="model">banas.hms.medicament.content</field>
            <field name="arch" type="xml">
                <tree string="HMS Medicament Content">
                    <field name="name" />
                </tree>
            </field>
        </record>
        <record id="view_banas_hms_medicament_content_form" model="ir.ui.view">
            <field name="name">banas.hms.medicament.content.form</field>
            <field name="model">banas.hms.medicament.content</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <form string="HMS Medication Dosage">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                            </group>
                            <group>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_banas_hms_medication_dosage_search" model="ir.ui.view">
            <field name="name">banas.hms.medication.dosage.search</field>
            <field name="model">banas.hms.medication.dosage</field>
            <field name="priority" eval="8" />
            <field name="arch" type="xml">
                <search string="HMS Medication Dosage">
                    <field name="abbreviation" />
                    <field name="code" />
                    <field name="name" />
                    <newline />
                    <!-- <group expand="0" string="Group By...">
                        <filter string="Abbreviation" domain="[]"
                            context="{'group_by':'abbreviation'}" />
                        <filter string="Code" domain="[]" context="{'group_by':'code'}" />
                        <filter string="Frequency" domain="[]" context="{'group_by':'name'}" />
                    </group> -->
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_open_medication_dosage_view">
            <field name="name">Medication Dosage</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">banas.hms.medication.dosage</field>
            <field name="view_mode">form</field>
            <field name="search_view_id" ref="view_banas_hms_medication_dosage_search" />
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <record model="ir.actions.act_window.view" id="act_open_medication_dosage_view_form">
            <field name="act_window_id" ref="act_open_medication_dosage_view" />
            <field name="sequence" eval="20" />
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_banas_hms_medication_dosage_form" />
        </record>

        <record model="ir.actions.act_window.view" id="act_open_medication_dosage_view_tree">
            <field name="act_window_id" ref="act_open_medication_dosage_view" />
            <field name="sequence" eval="10" />
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_banas_hms_medication_dosage_tree" />
        </record>

        <record model="ir.actions.act_window" id="action_medication_dosage">
            <field name="name">Medication Dosage</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">banas.hms.medication.dosage</field>
            <!-- <field name="view_type">form</field> -->
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>
        <record model="ir.actions.act_window" id="action_medicament_content">
            <field name="name">Medicament Content</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">banas.hms.medicament.content</field>
            <!-- <field name="view_type">form</field> -->
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <menuitem id="menu_banas_hms_medication_dosage"
            name="Medication Dosage"
            parent="banastech_hms.banas_hms_medicaments_conf_menu"
            action="action_medication_dosage"
            sequence="4" />
        <menuitem id="menu_banas_hms_medicament_content"
            name="Medicament Content"
            parent="banastech_hms.banas_hms_medicaments_conf_menu"
            action="action_medicament_content"
            sequence="5" />
    </data>
</odoo>